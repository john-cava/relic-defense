	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/relic_defense/scoreboardRow.js" />
	</scripts>
	<Panel class="scoreboardRow">
		<Panel class="scoreboardRowAvatar scoreboardRowItem">
			<DOTAAvatarImage id="scoreBoardRowImage"/>
		</Panel>
		<Panel class="scoreBoardRowName scoreboardRowItem">
			<Label id="scoreBoardRowNameLabel" text="xd" />
