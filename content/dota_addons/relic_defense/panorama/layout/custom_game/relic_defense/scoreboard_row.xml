		<Panel class="scoreBoardRowChampionKills scoreboardRowItem">
			<Label id="scoreBoardRowChampionKillsLabel" text="Champs" />
		</Panel>
		<Panel class="scoreBoardRowBossKills scoreboardRowItem">
			<Label id="scoreBoardRowBossKillsLabel" text="Boss" />
		</Panel>
		<Panel class="scoreBoardRowDeaths scoreboardRowItem">
			<Label id="scoreBoardRowDeathsLabel" text="Deaths" />
		</Panel>
